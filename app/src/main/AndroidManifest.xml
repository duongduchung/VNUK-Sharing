<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vn.edu.vnuk.vnuk_sharing">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Functional.FunctionalScreen" />
        <activity android:name=".Functional.SyllabusScreen" />
        <activity android:name=".Functional.AnnouncementsScreen" />
        <activity android:name=".Functional.DeadlinesScreen" />
        <activity android:name=".AfterLogin"></activity>
    </application>

</manifest>